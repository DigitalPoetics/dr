---
import BaseLayout from '../layouts/BaseLayout.astro';
import SearchWord2vec4 from '../components/SearchWord2vec4.astro';
// import data from '../data/deleuze/theDeleuzeSeminars.json'

import word2vecData from '../data/deleuze/deleuzeWord2vec.json';
import textData from '../data/deleuze/theDeleuzeSeminars.json';
// export const prerender = false;

const pageTitle = "Word2vec Visualization";
const pageDescription = "An interactive visualization of Word2Vec embeddings.";

// const fields = {
//   word: "word",
//   x: "x",
//   y: "y",
//   similar: "similar"
// }

const textFields = {
  author: "author",
  title: "title", 
  date: "date",
  url: "url",
  text: "text"
};
---
<BaseLayout pageTitle={pageTitle} description={pageDescription}>
  <SearchWord2vec4 
    word2vecData={word2vecData} 
    textData={textData}
    textFields={textFields} 
  />
</BaseLayout>

