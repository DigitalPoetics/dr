---
import BaseLayout from '../layouts/BaseLayout.astro';
import dataSrc from '../data/deleuze/deleuzeWord2vec.json';
import Word2vec from '../components/Word2vec.astro';
import SearchWord2vec2 from '../components/SearchWord2vec2.astro';

// export const prerender = false;

const data = '../data/deleuze/deleuzeWord2vec.json';
const pageTitle = "Word2vec Visualization";
const pageDescription = "An interactive visualization of Word2Vec embeddings.";

const fields = {
  word: "word",
  x: "x",
  y: "y",
  similar: "similar"
}
---
<BaseLayout pageTitle={pageTitle} description={pageDescription}>
    <SearchWord2vec2 data={dataSrc} fields={fields}/>
    <Word2vec fields={fields}/>
</BaseLayout>

