---
import BaseLayout from '../layouts/BaseLayout.astro';
import UiShowcase from '../components/ui/UiShowcase.astro';
const pageTitle = "Digital Tropology"
const description = "Of Stratas and Historical Formations"
---

<BaseLayout pageTitle={pageTitle} description={description}>
    <h3>Search UI</h3>
    <ul>
        <li>
            <a href="../searchUI?searchTerm=metanetwork" target="_blank">Search UI for L=A=N=G=U=A=G=E</a>
        </li>
        <li>
            <a href="../deleuzian1?searchTerm=mathematical+sublime" target="_blank">Search UI for the Deleuzian</a>
        </li>
        <li>
            <a href="../deleuzian2?searchTerm=diagram" target="_blank">Word2vec Viz for the Deleuzian</a>
        </li>
        <li>
            <a href="../deleuzian3?searchTerm=oceanic" target="_blank">Similarity Search UI for the Deleuzian</a>
        </li>
        <li>
            <a href="../deleuzian4?searchTerm=rhythm+refrain" target="_blank">Query Search UI for the Deleuzian</a>
        </li>
        <li>
            <a href="../deleuzian" target="_blank">Test</a>
        </li>
    </ul>
    <UiShowcase 
        pageTitle="Search UI"
        description="Spectrogram Visualization"
        imageUrl="/images/macro-rhythm.jpg"
        imageAlt="The Astro logo on a dark background with a pink glow"
        srcPath="/deleuzian1?searchTerm=mathematical+sublime"
    />
    <UiShowcase 
        pageTitle="Word2vec"
        description="Spectrogram Visualization"
        imageUrl="/images/macro-rhythm.jpg"
        imageAlt="The Astro logo on a dark background with a pink glow"
        srcPath="/deleuzian2?searchTerm=diagram"
    />
    <UiShowcase 
        pageTitle="Similarity Search"
        description="Spectrogram Visualization"
        imageUrl="/images/macro-rhythm.jpg"
        imageAlt="The Astro logo on a dark background with a pink glow"
        srcPath="/deleuzian3?searchTerm=diagram"
    />
    <UiShowcase 
        pageTitle="Query Search"
        description="Spectrogram Visualization"
        imageUrl="/images/macro-rhythm.jpg"
        imageAlt="The Astro logo on a dark background with a pink glow"
        srcPath="/deleuzian4?searchTerm=diagram+archive"
    />
</BaseLayout>